			pending_wi_obj[i].keysecondary = document.getElementById("wikeysec" + i).value;
			pending_wi_obj[i].keyanti = document.getElementById("wikeyanti" + i).value;
			pending_wi_obj[i].content = document.getElementById("wival" + i).value;
