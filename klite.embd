	/* Chat typing effect with flashing dots */
	.dot-flashing {
		position: relative;
		left: -15px;
		width: 8px;
		height: 8px;
		border-radius: 5px;
		background-color: #9e9e9e;
		color: #9e9e9e;
		animation: dot-flashing 1s infinite linear alternate;
		animation-delay: 0.5s;
	}
	.dot-flashing::before, .dot-flashing::after {
		content: "";
		display: inline-block;
		position: absolute;
		top: 0;
	}
	.dot-flashing::before {
		left: -15px;
		width: 8px;
		height: 8px;
		border-radius: 5px;
		background-color: #9e9e9e;
		color: #9e9e9e;
		animation: dot-flashing 1s infinite alternate;
		animation-delay: 0s;
	}
	.dot-flashing::after {
		left: 15px;
		width: 8px;
		height: 8px;
		border-radius: 5px;
		background-color: #9e9e9e;
		color: #9e9e9e;
		animation: dot-flashing 1s infinite alternate;
		animation-delay: 1s;
	}
	@keyframes dot-flashing {
		0% {
			background-color: #9e9e9e;
		}
		29.9% {
			background-color: #9e9e9e;
		}
		30%, 100% {
			background-color: #9e9e9e33;
		}
	}

