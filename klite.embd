	function decompress_story(cstoryjson) //decompress story from LZMA to json object
	{
		var story = null;
		try
		{
			var storyjson = lz_d.decompress(b64_to_buf(cstoryjson));
			if (storyjson == null || storyjson == "") {
				return null;
			} else {
				console.log("Decompressed story: " + storyjson);
