				{
					tempAltGreetings.push(obj.first_mes);
				}
				tempAltGreetings = tempAltGreetings.concat(obj.alternate_greetings);
				tempAltGreetings = tempAltGreetings.slice(0,16);
				let bufMsg = "<p><b><u>Select Greeting Message</u></b></p>"
